{
    "manifest_version": 2,
    "name": "Interview Google Meet",
    "version": "0.2",

    "icons": { "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
       "128": "icons/icon128.jpg" },

    "content_scripts":[
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js":["index.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Interview Google Meet"
    },
    "permissions": ["contextMenus"]
  }