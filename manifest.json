{
    "manifest_version": 2,
    "name": "Interview Google Meet",
    "version": "0.1",

    "content_scripts":[
        {
            "matches": [
                "https://meet.google.com/*"
            ],
            "js":["index.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Interview Google Meet"
    },
    "permissions": ["contextMenus"]
  }